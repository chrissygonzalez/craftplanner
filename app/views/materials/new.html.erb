<div class="container">
    <div class="form-card mt-50">
<h1>New Material</h1>

<%= render partial: "layouts/errors", locals: {object: @material} %>

<%= form_for @material do |f| %>
    <%= f.label "Name", class: "label" %>
    <%= f.text_field :name, class: "width-60" %>
    <br>
  <%= f.submit %>
<% end %>
</div>
</div>